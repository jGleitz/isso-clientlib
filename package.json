{
  "name": "isso-clientlib",
  "version": "0.1.0",
  "description": "A client library for the isso commenting server.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/*"
  ],
  "scripts": {
    "typecheck": "tsc --build .",
    "lint": "prettylint '**/*.{js,ts,json,yml,md}' && eslint --max-warnings=0 '**/*.{js,ts}'",
    "format": "prettylint --fix '**/*.{js,ts,json,yml,md}' && eslint --max-warnings=0 --fix '**/*.{js,ts}'",
    "start": "jest -c unit.jest.config.js --watch",
    "unit-test": "jest -c unit.jest.config.js",
    "integration-test": "jest -c integration.jest.config.js",
    "test": "npm run lint && npm run unit-test && npm run integration-test",
    "doc": "typedoc lib",
    "build-dist": "npx babel lib --extensions .ts --out-dir dist && tsc --build lib",
    "prepack": "rm -rf dist && npm run build-dist"
  },
  "author": "Joshua Gleitze",
  "repository": "https://github.com/jGleitz/isso-clientlib.git",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.7",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.8.7",
    "@babel/preset-typescript": "^7.8.3",
    "@types/clone": "^0.1.30",
    "@types/jest": "25.1.4",
    "@types/mkdirp": "^1.0.0",
    "@types/rimraf": "^2.0.2",
    "@types/superagent": "^4.0.0",
    "@types/tmp": "^0.1.0",
    "@typescript-eslint/eslint-plugin": "^2.23.0",
    "@typescript-eslint/eslint-plugin-tslint": "^2.23.0",
    "@typescript-eslint/parser": "^2.23.0",
    "chalk": "^3.0.0",
    "clone": "^2.0.0",
    "eslint": "^6.8.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-tsdoc": "^0.2.3",
    "jest": "25.1.0",
    "jest-cli": "25.1.0",
    "jest-extended": "0.11.5",
    "prettier": "^1.19.1",
    "prettylint": "^1.0.0",
    "rimraf": "^3.0.0",
    "tmp-promise": "^2.0.2",
    "ts-jest": "25.2.1",
    "ts-node": "8.6.2",
    "tslint": "^6.1.0",
    "typed-promisify": "^0.4.0",
    "typedoc": "^0.16.0",
    "typescript": "3.8.3"
  },
  "dependencies": {
    "superagent": "^5.0.0",
    "ts-events": "^3.2.1"
  }
}
