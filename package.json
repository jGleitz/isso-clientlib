    "@typescript-eslint/eslint-plugin": "^2.23.0",
    "@typescript-eslint/eslint-plugin-tslint": "^2.23.0",
    "@typescript-eslint/parser": "^2.23.0",
    "chalk": "^3.0.0",
    "clone": "^2.0.0",
    "eslint": "^6.8.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-tsdoc": "^0.2.3",
    "jest": "^24.1.0",
    "jest-cli": "^24.3.1",
    "jest-extended": "^0.11.1",
    "prettier": "^1.19.1",
    "prettylint": "^1.0.0",
    "rimraf": "^3.0.0",
    "ts-jest": "^25.0.0",
    "ts-node": "^8.0.2",
    "tslint": "^6.1.0",
    "typed-promisify": "^0.4.0",
    "typedoc": "^0.16.0",
    "typescript": "3.8.3"
