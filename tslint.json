{
	"rules": {
		"align": [
			true,
			"statements"
		],
		"ban-comma-operator": true,
		"class-name": true,
		"comment-format": [
			true,
			"check-space"
		],
		"curly": true,
		"eofline": true,
		"indent": [true, "tabs"],
		"label-position": true,
		"max-line-length": [true, 120],
		"member-access": true,
		"new-parens": true,
		"no-arg": true,
		"no-bitwise": true,
		"no-construct": true,
		"no-debugger": true,
		"no-duplicate-imports": true,
		"no-duplicate-variable": true,
		"no-empty": true,
		"no-eval": true,
		"no-implicit-dependencies": true,
		"no-internal-module": true,
		"no-invalid-this": true,
		"no-namespace": true,
		"no-parameter-reassignment": true,
		"no-reference": true,
		"no-redundant-jsdoc": true,
		"no-require-imports": true,
		"no-shadowed-variable": true,
		"no-string-literal": true,
		"no-switch-case-fall-through": true,
		"no-trailing-whitespace": true,
		"no-unused-expression": true,
		"no-unused-variable": true,
		"no-use-before-declare": true,
		"no-var-keyword": true,
		"no-var-requires": true,
		"one-line": [
			true,
			"check-catch",
			"check-finally",
			"check-else",
			"check-open-brace",
			"check-whitespace"
		],
		"prefer-const": true,
		"prefer-readonly": true,
		"quotemark": [
			true,
			"single"
		],
		"radix": true,
		"semicolon": [
			true,
			"always"
		],
		"space-within-parens": [
			true,
			0
		],
		"trailing-comma": {
			"multiline": "never",
			"singleline": "never"
		},
		"triple-equals": true,
		"typedef": [
			true,
			"call-signature",
			"parameter",
			"property-declaration"
		],
		"typedef-whitespace": true,
		"use-isnan": true,
		"variable-name": [
			true,
			"check-format",
			"ban-keywords",
			"allow-leading-underscore"
		],
		"whitespace": [
			true,
			"check-branch",
			"check-decl",
			"check-operator",
			"check-module",
			"check-separator",
			"check-type"
		]
	}
}
